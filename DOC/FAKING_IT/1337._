            //:#_CALCULATE_FPSBIN_#:[1_337]:-------------://     // [1_337][-----------------]
                                                                 // [1_337][-----------------]
                let fpsbin5 =(( 0x000 //:----------------://     // [1_337][-----------------]
                | ( d_fps_005 << 4 )  //:                ://     // [1_337][-----------------]
                | ( d_fps_015 << 3 )  //: What Systems   ://     // [1_337][-----------------]
                | ( d_fps_030 << 2 )  //: executed last  ://     // [1_337][-----------------]
                | ( d_fps_060 << 1 )  //: time ?         ://     // [1_337][-----------------]
                | ( d_fps_120 << 0 )  //:                ://     // [1_337][-----------------]
                )>>>0);;              //:----------------://     // [1_337][-----------------]
                                                                 // [1_337][-----------------]
                const     takemil =(     d_cligame_mil );;;;     // [1_337][-----------------]
            //- d_fpsbin5_takemil[ fpsbin5 ]=( takemil );-//     // [1_337][-----------------]
                                                                 // [1_337][-----------------]
            //:-------------:#_CALCULATE_FPSBIN_#:[1_337]://     // [1_337][-----------------]
            //:#_MIL_ACCUMULATORS_#:[1_337]:-------------://     // [1_337][-----------------]
            //:#_FPS_ACCUMULATORS_#:[1_337]:-------------://     // [1_337][-----------------]
                                                                 // [1_337][-----------------]
                d_mil_005 +=( d_cligame_mil );                   // [1_337][-----------------]
                d_mil_015 +=( d_cligame_mil );                   // [1_337][-----------------]
                d_mil_030 +=( d_cligame_mil );                   // [1_337][-----------------]
                d_mil_060 +=( d_cligame_mil );                   // [1_337][-----------------]
                d_mil_120 +=( d_cligame_mil );                   // [1_337][-----------------]
                                                                 // [1_337][-----------------]
            //:-------------:#_FPS_ACCUMULATORS_#:[1_337]://     // [1_337][-----------------]
            //:-------------:#_MIL_ACCUMULATORS_#:[1_337]://     // [1_337][-----------------]




    #_CALCULATE_FPSBIN_# :------------------------------------:  // [1_337]
                                                              |  // [1_337]
        Create a unique binary value based on which           |  // [1_337]
        sub-systems executed last frame. We want to           |  // [1_337]
        use this unique binary value to make an               |  // [1_337]
        [ elapsed_time_entry ]in the                          |  // [1_337]
        [ taken_milliseconds_table ].                         |  // [1_337]
                                                              |  // [1_337]
        @d_fpsbin5_takemil@ : taken_milliseconds_table        |  // [1_337]
                            : (fpsbin5)===>(takemil)          |  // [1_337]
                            : taken_ms_table                  |  // [1_337]
                            : taken_millisecond_table         |  // [1_337]
                                                              |  // [1_337]
        The time entries putt into the[ taken_ms_table ]      |  // [1_337]
        allow us to make an educated guess on how long        |  // [1_337]
        it is going to take to execut the next frame.         |  // [1_337]
                                                              |  // [1_337]
        Since our fastest frame rate is[ 120_FPS ],           |  // [1_337]
        any[ predicted_execution_time ]taken from             |  // [1_337]
        [ fpsbin5_takemil / taken_millisecond_table ]         |  // [1_337]
        that is[ OVER_1000_DIVIDED_BY_120 ]will mean          |  // [1_337]
        [ DO_NOT_DELAY_AND_EXECUTE_NEXT_TICK_NOW ].           |  // [1_337]
                                                              |  // [1_337]
    :------------------------------------: #_CALCULATE_FPSBIN_#  // [1_337]
    #_MIL_ACCUMULATORS_# :------------------------------------:  // [1_337]
    #_FPS_ACCUMULATORS_# :------------------------------------:  // [1_337]
                                                              |  // [1_337]
        Adjust millisecond accumulators.                      |  // [1_337]
        I also referred to them as "fps_accumulators"         |  // [1_337]
        but it is more correct to call them                   |  // [1_337]
        [ millisecond_accumulators ].                         |  // [1_337]
                                                              |  // [1_337]
        We have[ 5_MILLISECOND_ACCUMULATORS_TOTAL ].          |  // [1_337]
        One for each[ FRAME_RATE_TARGET ]we want to           |  // [1_337]
        pin different[ sub_system_execution ]                 |  // [1_337]
        frame_rates to.                                       |  // [1_337]
                                                              |  // [1_337]
    :------------------------------------: #_FPS_ACCUMULATORS_#  // [1_337]
    :------------------------------------: #_MIL_ACCUMULATORS_#  // [1_337]